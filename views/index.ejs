<!DOCTYPE html>
<html>
  <head>
    <%- include("partials/head.ejs")%>
  </head>
  <body>
    <div class="headerContainer">
      <%- include("partials/header.ejs")%>
    </div>
    
    <main>

      <h2>Contenidos</h2>

      <div class="mainContent">
        <% itemP.forEach(item => { %>

          <a href="/entrada/<%- item.id %>" id="<%- item.id %>">
          <figure class="entradaThumbnail">

              <img src="<%- item.thumbnail %>" alt="<%= item.title %>">
              <figcaption><%- item.intro %> <span></figcaption>

          </figure>        
        </a>
         <% }) %>  
        </div>

    </main>
    <footer>
      <%- include("partials/footer.ejs")%>
    </footer>
  </body>
</html>
